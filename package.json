{
  "name": "gisto",
  "description": "Cross-platform snippets management desktop application",
  "productName": "Gisto",
  "private": true,
  "version": "2.4.3",
  "type": "module",
  "scripts": {
    "dev": "vite --port 61570",
    "build": "tsc -b && vite build",
    "lint": "eslint ./src",
    "ts:lint": "tsc -b --noEmit",
    "preview": "vite preview",
    "prepare": "husky",
    "commit": "cz",
    "changelog": "npx git-cliff@latest --output CHANGELOG.md",
    "version": "pnpm run changelog && git add CHANGELOG.md",
    "release:major": "pnpm run version:major && pnpm run changelog && pnpm run commit:changelog",
    "version:major": "pnpm version major",
    "release:minor": "pnpm run version:minor && pnpm run changelog && pnpm run commit:changelog",
    "version:minor": "pnpm version minor",
    "release:patch": "pnpm run version:patch && pnpm run changelog && pnpm run commit:changelog",
    "version:patch": "pnpm version patch",
    "commit:changelog": "git add CHANGELOG.md && git commit -m \"chore: release $npm_package_version\" && git push && git push --tags --no-verify",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@commitlint/types": "^20.0.0",
    "@mdit/plugin-alert": "^0.22.3",
    "@mdit/plugin-tasklist": "^0.22.2",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tauri-apps/plugin-dialog": "~2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-opener": "~2.5.2",
    "@tauri-apps/plugin-process": "~2.3.1",
    "@tauri-apps/plugin-updater": "~2.9.0",
    "@tauri-apps/plugin-window-state": "~2.4.1",
    "class-variance-authority": "^0.7.1",
    "dirty-react-router": "^0.1.0",
    "gisto": "link:",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.25",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.553.0",
    "markdown-it": "^14.1.0",
    "motion": "^12.23.24",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropdown-select": "^4.12.2",
    "react-leaflet": "^5.0.0",
    "recharts": "^2.15.4",
    "swagger-ui-react": "^5.30.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tauri-apps/cli": "^2.9.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/leaflet": "^1.9.21",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.10.1",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "4.0.9",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*": [
      "prettier --ignore-unknown --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes"
        },
        "docs": {
          "description": "Documentation only changes",
          "title": "Documentation"
        },
        "style": {
          "description": "Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc.)",
          "title": "Styles"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring"
        },
        "perf": {
          "description": "A code change that improves performance",
          "title": "Performance Improvements"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Tests"
        },
        "build": {
          "description": "Changes that affect the build system or external dependencies (example scopes: gulp, broccoli, npm)",
          "title": "Builds"
        },
        "ci": {
          "description": "Changes to our CI configuration files and scripts (example scopes: Travis, Circle, BrowserStack, SauceLabs)",
          "title": "Continuous Integrations"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores"
        },
        "revert": {
          "description": "Reverts a previous commit",
          "title": "Reverts"
        },
        "typo": {
          "description": "Fixing typos in documentation or code comments",
          "title": "Typo"
        },
        "i18n": {
          "description": "Internationalization and translation updates",
          "title": "Translations"
        }
      }
    }
  },
  "pnpm": {
    "overrides": {
      "prismjs@<1.30.0": ">=1.30.0",
      "tmp@<=0.2.3": ">=0.2.4",
      "js-yaml@<4.1.1": ">=4.1.1"
    }
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "engines": {
    "node": ">=24"
  }
}
